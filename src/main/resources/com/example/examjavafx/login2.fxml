<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="300.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examjavafx.Login2Controller">
    <children>
        <TextField fx:id="usernameText" layoutX="234.0" layoutY="126.0" />
        <Label fx:id="mySpaceLabel" layoutX="169.0" layoutY="37.0" text="My Space">
            <font>
                <Font name="Bernard MT Condensed" size="48.0" />
            </font>
        </Label>
        <PasswordField fx:id="passwordText" layoutX="234.0" layoutY="181.0" />
        <Label layoutX="125.0" layoutY="183.0" text="Password">
            <font>
                <Font name="Berlin Sans FB" size="18.0" />
            </font>
        </Label>
        <Button layoutX="225.0" layoutY="233.0" mnemonicParsing="false" onAction="#onLoginButtonClick" text="Login">
            <font>
                <Font name="Arial Rounded MT Bold" size="12.0" />
            </font></Button>
        <Label layoutX="122.0" layoutY="128.0" text="Username">
            <font>
                <Font name="Berlin Sans FB" size="18.0" />
            </font>
        </Label>
        <MenuBar layoutX="403.0" layoutY="14.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="closeMenuButton" mnemonicParsing="false" onAction="#onCloseMenuButtonClick" text="Close" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <Label fx:id="promptText" layoutX="307.0" layoutY="264.0" prefHeight="27.0" prefWidth="233.0" textFill="#c10000">
            <font>
                <Font name="System Bold" size="18.0" />
            </font></Label>
    </children>
</AnchorPane>
